{
  "name": "runn/framework",
  "description": "Runn Me! Framework",
  "keywords": ["runn", "runn.me", "framework"],
  "type": "framework",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Albert Stepantsev"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "psr/simple-cache": "^1.0",
    "runn/core": "dev-master",
    "runn/http": "dev-master",
    "runn/fs": "dev-master",
    "runn/serialization": "dev-master",
    "symfony/dotenv": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*"
  },
  "autoload": {
    "psr-4": {
      "Runn\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  }
}
